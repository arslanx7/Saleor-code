name: Deploy to AKS

on:
  push:
    branches: [ main ]
  pull_request:
    branches: [ main ]

  workflow_dispatch:

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      
      - uses: azure/docker-login@v1
        with:
          login-server: saleortest2.azurecr.io
          username: ${{ secrets.ACR_username }}
          password: ${{ secrets.ACR_password }}
      
      - run: |
            
